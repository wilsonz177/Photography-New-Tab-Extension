{
 "manifest_version": 2,
 "name": "Photo of the Day",
 "description": "A photograph of the day in your default tab",
 "version": "1",
 "author": "Wilson Zhong",
"browser_action": {
   // "default_icon": "tab-icon.png",
   "default_title": "Have a good day",
   "default_popup": "popup.html"
 },
"chrome_url_overrides" : {
  "newtab": "newtab.html"
},
 "permissions": [
 	"activeTab",
    "https://*/",
    "storage"
 ]
}
